    "def  infinit_chai():\n",
    "    count=1\n",
    "    while True:\n",
    "        yield f\"refill {count}\"\n",
    "        count+=1\n",
    "refill=infinit_chai()\n",
    "for _ in range(5):\n",
    "    print(next(refill))"
   ]
